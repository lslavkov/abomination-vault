mon","custom":""},"rules":[],"slug":"grim-tendrils","schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:52.375+01:00","version":{"schema":0.716,"foundry":"9.249","system":"3.4.3.10468"}}},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":30,"areaType":"line"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"negative"},"value":"2d4"}}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"pMf4lweHa7ni4nWq"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"30-foot line"},"prepared":{"value":""},"primarycheck":{"value":""},"scaling":{"damage":{"0":"2d4"},"interval":1},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.k34hDOfIIMAxNL4a"}},"img":"systems/pf2e/icons/spells/grim-tendrils.webp","name":"Grim Tendrils","sort":2200000,"type":"spell","folder":null,"permission":{"default":0}},{"_id":"NF3p9lsrzts1vIoK","data":{"description":{"value":"<p>The object glows, casting bright light in a 20-foot radius (and dim light for the next 20 feet) like a torch. If you cast this spell again on a second object, the light spell on the first object ends.</p>\n<hr />\n<p><strong>Heightened (4th)</strong> The object sheds bright light in a 60-foot radius (and dim light for the next 60 feet).</p>"},"source":{"value":""},"traits":{"value":["light","cantrip"],"rarity":"common","custom":""},"rules":[],"slug":"light","schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:52.375+01:00","version":{"schema":0.716,"foundry":"9.249","system":"3.4.3.10