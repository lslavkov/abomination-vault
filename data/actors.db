om/Spells.aspx?ID=164"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:48.435+01:00","version":{"schema":0.716,"foundry":"9.249","system":"3.4.3.10468"}}},"level":{"value":2},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":true,"somatic":true,"verbal":false},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"Z60YSknQUrMbR4mB"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"prepared":{"value":""}},"effects":[],"flags":{},"img":"systems/pf2e/icons/spells/invisibility.webp","name":"Invisibility","sort":500000,"type":"spell","folder":null,"permission":{"default":0}},{"_id":"wkCF4VWAy10sw2NP","data":{"description":{"value":"<p>You conjure an animal to fight for you. You summon a common creature that has the animal trait and whose level is -1. Heightening the spell increases the maximum level of creature you can summon.</p>\n<hr />\n<p><strong>Heightened</strong> Level 1</p>\n<p><strong>Heightened</strong> Level 2</p>\n<p><strong>Heightened</strong> Level 3</p>\n<p><strong>Heightened</strong> Level 5</p>\n<p><strong>Heightened</strong> Level 7</p>\n<p><strong>Heightened</strong> Level 9</p>\n<p><strong>Heightened</strong> Level 11</p>\n<p><strong>Heightened</strong> Level 13</p>\n<p><strong>Heightened</strong> Level 15</p>"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=316"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:48.435+01:00","version":{"schema"