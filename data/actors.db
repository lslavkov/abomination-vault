nds]]{1d4 rounds}, as it is too glutted on fear to suppress its Glow.</p>"},"source":{"value":""},"traits":{"value":["concentrate"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:52.372+01:00","version":{"schema":0.716,"foundry":"9.249","system":"3.4.3.10468"}}},"actionType":{"value":"action"},"actionCategory":{"value":"offensive"},"actions":{"value":1},"requirements":{"value":""},"trigger":{"value":""},"weapon":{"value":""}},"effects":[],"flags":{},"img":"systems/pf2e/icons/actions/OneAction.webp","name":"Feed on Fear","sort":1000000,"type":"action","folder":null,"permission":{"default":0}},{"_id":"csb4buUROte3sD1C","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:52.372+01:00","version":{"schema":0.716,"foundry":"9.249","system":"3.4.3.10468"}}},"mod":{"value":18},"proficient":{"value":0},"item":{"value":0}},"effects":[],"flags":{},"img":"systems/pf2e/icons/default-icons/lore.svg","name":"Acrobatics","sort":1100000,"type":"lore","folder":null,"permission":{"default":0}},{"_id":"G7O4AjlCNrZXmVPX","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:52.372+01:00","version":{"schema":0.716,"foundry":"9.249","system":"3.4.3.10468"}}},"mod":{"value":15},"proficient":{"value":0},"item":{"value":0}},"effects":[],"flags":{},"img":"systems/pf2e/icons/default-icons/lore.svg","name":"Deception","sort":1200000,"type":"lore","folder":null,"permission":{"default":0}},{"_id":"wRKLdw4G3XTGMMQY","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.719,"lastMigration":{"datetime":"2022-02-17T23:00:52.372+